{
  "stack": "nodejs",
  "memory": 1024,
  "ttl": 300,
  "timeout": 60,
  "cpu": {
    "type": "shared",
    "value": 5,
    "scalingPercentage": 20
  },
  "minReplicas": 8,
  "maxReplicas": 80,
  "workers": 1,
  "routes": {
    "orders": {
      "path": "/b2b/oms/user/orders/",
      "public": true
    },
    "checkout": {
      "path": "/b2b/oms/user/checkout/",
      "public": true
    },
    "order": {
      "path": "/b2b/oms/user/orders/:orderId",
      "public": true
    }
  },
  "events": {
    "onAppInstalled": {
      "sender": "apps",
      "keys": ["setup"]
    },
    "onAppLinked": {
      "sender": "apps",
      "keys": ["linked", "relinked"]
    }
  }
}
